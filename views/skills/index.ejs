<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express Developer Skills</title>
</head>
<body>
  <a href="/skills">Skill List</a>
  <h1>Skills</h1>
  <% if(error) { %>
    <h2><%= error %></h2>
  <% } %>
  <ul>
    <% todos.forEach(function(todo) { %>
      <li>
        <%= todo.text %>
          - 
        <%= todo.done ? 'done' : 'not done' %>
      </li>
    <% }) %>
  </ul>
</body>
</html>